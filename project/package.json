{
  "name": "meitq-vue3-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --ignore-pattern /src/assets/icons/*",
    "format": "prettier --write src/",
    "prepare": "husky"
  },
  "dependencies": {
    "@turf/turf": "~6.5.0",
    "@vueuse/core": "~10.10.0",
    "dayjs": "~1.11.11",
    "echarts": "~5.5.0",
    "element-plus": "~2.7.4",
    "file-saver": "~2.0.5",
    "html2canvas": "~1.4.1",
    "jszip": "~3.10.1",
    "lodash-es": "^4.17.21",
    "normalize.css": "~8.0.1",
    "nprogress": "~0.2.0",
    "pinia": "~2.1.7",
    "pinia-plugin-persistedstate": "~3.2.1",
    "vue": "~3.4.27",
    "vue-router": "~4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "~19.3.0",
    "@commitlint/config-conventional": "~19.2.2",
    "@rushstack/eslint-patch": "~1.10.3",
    "@tsconfig/node20": "~20.1.4",
    "@types/file-saver": "~2.0.7",
    "@types/lodash-es": "~4.17.12",
    "@types/node": "~20.14.1",
    "@types/nprogress": "~0.2.3",
    "@vitejs/plugin-vue": "~5.0.5",
    "@vue/eslint-config-prettier": "~9.0.0",
    "@vue/eslint-config-typescript": "~13.0.0",
    "@vue/tsconfig": "~0.5.1",
    "eslint": "~8.57.0",
    "eslint-plugin-vue": "~9.26.0",
    "husky": "~9.0.11",
    "npm-run-all": "~4.1.5",
    "prettier": "~3.3.0",
    "sass": "~1.77.4",
    "typescript": "~5.4.5",
    "unplugin-auto-import": "~0.17.6",
    "unplugin-vue-components": "~0.27.0",
    "vite": "~5.2.12",
    "vite-plugin-compression2": "~1.1.1",
    "vue-tsc": "~2.0.19"
  }
}
