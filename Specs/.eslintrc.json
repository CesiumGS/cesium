{
    "extends": "../Source/.eslintrc.json",
    "env": {
        "jasmine": true
    },
    "rules": {
        "no-self-assign": "off",
        "es/no-block-scoped-variables": "off",
        "es/no-template-literals": "off"
    },
    "overrides": [
        {
            "files": [
                "karma.conf.cjs"
            ],
            "parserOptions": {
                "sourceType": "script"
            }
        }
    ]
}
